import java.util.HashMap;
import java.util.List;
import java.util.Map;
import java.util.Map.Entry;
import java.util.stream.Collectors;

public class TestEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
      /*Convert Map<String, Integer> to List<String> where each element in the list follows "<key>,<value>"
For example: If Key = "A" and Value = 1, then the element is "A,1"
*/
		
		Map<String, Integer> map= new HashMap<String, Integer>();
		   map.put("A", 1);
		 map.entrySet().stream().collect(Collectors.toList()).forEach(System.out::println);
	}

}
